{
  "name": "dinner-stranger-booking",
  "version": "1.0.0",
  "description": "Book your seat and meet 5 strangers over dinner - all matched by our personality algorithm",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest"
  },
  "keywords": [
    "dinner",
    "social",
    "matching",
    "networking",
    "personality"
  ],
  "author": "ramji3030",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "stripe": "^13.5.0",
    "axios": "^1.5.0",
    "validator": "^13.11.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "@types/node": "^20.5.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
